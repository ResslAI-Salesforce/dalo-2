{
  "name": "@openclaw/email",
  "version": "2026.2.9",
  "private": true,
  "description": "OpenClaw Email (Gmail) channel plugin",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "html-to-text": "^9.0.5",
    "mailparser": "^3.7.2"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/mailparser": "^3.4.5",
    "openclaw": "workspace:*",
    "typescript": "^5.5.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "channel": {
      "id": "email",
      "label": "Email (Gmail)",
      "selectionLabel": "Email (Gmail plugin)",
      "docsPath": "/channels/email",
      "docsLabel": "email",
      "blurb": "Gmail-based email channel; install the plugin to enable.",
      "order": 70
    },
    "install": {
      "npmSpec": "@openclaw/email",
      "localPath": "extensions/email",
      "defaultChoice": "npm"
    }
  }
}
